{
    "name": "tipoff/profiles",
    "description": "Laravel Package for Polymorphic Profiles",
    "keywords": [
        "tipoff",
        "profiles"
    ],
    "homepage": "https://github.com/tipoff/profiles",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "tipoff/authorization": "^2.5.4",
        "tipoff/support": "^1.6.2",
        "tipoff/seo": "^2.2.0"
    },
    "require-dev": {
        "tipoff/test-support": "^1.0.8"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\Profiles\\": "src",
            "Tipoff\\Profiles\\Database\\Factories\\": "database/factories",
            "Tipoff\\Profiles\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\Profiles\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\Profiles\\ProfilesServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
